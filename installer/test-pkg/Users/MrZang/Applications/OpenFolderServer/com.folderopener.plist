<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.folderopener</string>
    
    <key>ProgramArguments</key>
    <array>
        <string>USER_HOME/Applications/OpenFolderServer/start.command</string>
    </array>
    
    <key>WorkingDirectory</key>
    <string>USER_HOME/Applications/OpenFolderServer</string>
    
    <key>RunAtLoad</key>
    <true/>
    
    <key>KeepAlive</key>
    <true/>
    
    <key>StandardOutPath</key>
    <string>USER_HOME/Applications/OpenFolderServer/logs/launchd.out</string>
    
    <key>StandardErrorPath</key>
    <string>USER_HOME/Applications/OpenFolderServer/logs/launchd.err</string>
    
    <key>EnvironmentVariables</key>
    <dict>
        <key>PATH</key>
        <string>/opt/homebrew/bin:/usr/local/bin:/usr/bin:/bin</string>
    </dict>
</dict>
</plist>